<odoo>
    <data>
        <template id="homepage_template" name="Homepage">
            <t t-call="website.layout">
                <t t-jquery="head" t-operation="append">
                    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css"
                          rel="stylesheet"/>
                </t>
                <xpath expr="//main" position="before">
                    <style>
                        body {
                        font-family: Arial, sans-serif;
                        margin: 0;
                        padding: 0;
                        background-color: white;
                        }
                        .about-mda-section {
                        background-color: #D3D3D3; /* Light gray background */
                        }
                        .about-title {
                        font-size: 1.8rem;
                        font-weight: bold;
                        color: #333;
                        }
                        .about-title .highlight {
                        color: #0056b3; /* Blue color for emphasis */
                        }
                        .about-text {
                        font-size: 1rem;
                        color: #555;
                        line-height: 1.6;
                        margin-bottom: 1rem;
                        }
                        .read-more {
                        display: inline-block;
                        font-size: 1rem;
                        font-weight: bold;
                        color: #0056b3;
                        text-decoration: none;
                        border-bottom: 2px solid #0056b3;
                        transition: color 0.3s, border-color 0.3s;
                        }
                        .read-more:hover {
                        color: #003d80;
                        border-color: #003d80;
                        }
                        .services-section {
                        background-color:white;
                        color: #ffffff; /* White text */
                        }
                        .service-card {
                        background-color: #034673;
                        padding: 20px;
                        border: 1px solid #004080;
                        border-radius: 10px;
                        margin: 10px 0;
                        transition: transform 0.3s, box-shadow 0.3s;
                        }
                        .service-card:hover {
                        transform: scale(1.05);
                        box-shadow: 0 8px 16px rgba(0, 0, 0, 0.2);
                        }
                        .service-icon {
                        font-size: 3rem;
                        color: #ffffff;
                        margin-bottom: 15px;
                        }
                        .service-title {
                        font-size: 1.2rem;
                        font-weight: bold;
                        margin-bottom: 10px;
                        }
                        .service-description {
                        font-size: 0.9rem;
                        line-height: 1.6;
                        }
                        .service-card i {
                        display: inline-block;
                        margin-bottom: 10px;
                        }
                        .stats-section {
                        display: flex;
                        justify-content: center;
                        align-items: center;
                        gap: 40px; /* Space between cards */
                        padding: 50px 20px;
                        background-color: #D3D3D3; /* Light grey background */
                        }
                        .stat-card {
                        text-align: center;
                        width:250px;
                        padding-top:20px;
                        padding-bottom:20px;
                        color: #b3b3b3; /* Light grey color for text */
                        }
                        .stat-card:hover {
                        background-color:#034673;
                        color:white;
                        }
                        .stat-card .icon {
                        font-size: 50px; /* Large icon size */
                        color: #00bcd4; /* Primary color for icons */
                        margin-bottom: 15px;
                        }
                        .stat-number {
                        font-size: 36px;
                        font-weight: bold;
                        }
                        .stat-label {
                        font-size: 18px;
                        text-transform: lowercase;
                        letter-spacing: 1px;
                        font-weight: bold;
                        }


                    </style>
                </xpath>
                <div class="">
                    <div id="imageCarousel" class="carousel slide" data-bs-ride="carousel">
                        <!-- Indicators -->
                        <div class="carousel-indicators">
                            <button type="button" data-bs-target="#imageCarousel" data-bs-slide-to="0" class="active"
                                    aria-current="true" aria-label="Slide 1"/>
                            <button type="button" data-bs-target="#imageCarousel" data-bs-slide-to="1"
                                    aria-label="Slide 2"/>
                            <button type="button" data-bs-target="#imageCarousel" data-bs-slide-to="2"
                                    aria-label="Slide 3"/>
                        </div>

                        <!-- Carousel Inner -->
                        <div class="carousel-inner">
                            <div class="carousel-item active">
                                <img src="https://www.mda.services/wp-content/uploads/2017/08/ban2.jpg"
                                     style="height:400px;" class="d-block w-100" alt="Image 1"/>
                                <div class="carousel-caption d-none d-md-block">

                                </div>
                            </div>
                            <div class="carousel-item">
                                <img src="https://www.mda.services/wp-content/uploads/2017/08/bg1.jpg"
                                     style="height:400px;" class="d-block w-100" alt="Image 2"/>
                                <div class="carousel-caption d-none d-md-block">
                                    <h5>Image 2</h5>
                                    <p>Description for image 2.</p>
                                </div>
                            </div>
                            <div class="carousel-item">
                                <img src="https://www.mda.services/wp-content/uploads/2017/08/bg1.jpg"
                                     style="height:400px;" class="d-block w-100" alt="Image 3"/>
                                <div class="carousel-caption d-none d-md-block">
                                    <h5>Image 3</h5>
                                    <p>Description for image 3.</p>
                                </div>
                            </div>
                        </div>

                        <!-- Controls -->
                        <button class="carousel-control-prev" type="button" data-bs-target="#imageCarousel"
                                data-bs-slide="prev">
                            <span class="carousel-control-prev-icon" aria-hidden="true"/>
                            <span class="visually-hidden">Previous</span>
                        </button>
                        <button class="carousel-control-next" type="button" data-bs-target="#imageCarousel"
                                data-bs-slide="next">
                            <span class="carousel-control-next-icon" aria-hidden="true"/>
                            <span class="visually-hidden">Next</span>
                        </button>
                    </div>
                </div>
                <section class="about-mda-section py-5">
                    <div class="container">
                        <div class="row align-items-center">
                            <!-- Image Section -->
                            <div class="col-md-6">
                                <img src="data:image/jpeg;base64,"
                                     alt="About MDA" class="img-fluid rounded" style="width:600px"/>
                            </div>
                            <!-- Text Section -->
                            <div class="col-md-6">
                                <h2 class="about-title">ABOUT
                                    <span class="highlight">MDA</span>
                                </h2>
                                <p class="about-text">
                                    Modern Digital Applications is a company that wants to help you redefine your
                                    digital presence. Whether you are an individual, a business or a government agency,
                                    MDA offers the strategy and tools you need to deliver your clients a superior
                                    lifestyle, a more competitive business, and ready-to-deliver government services.
                                </p>
                                <p class="about-text">
                                    Living in the era of digital communications means combining the tools of information
                                    technology with current online channels of communications – empowering you to become
                                    stronger, faster, and more competitive. In short, to get ahead of your game.
                                </p>
                                <a href="#" class="read-more" style="text-decoration:none;">READ MORE</a>
                            </div>
                        </div>
                    </div>
                </section>

                <section class="services-section py-5">
                    <div class="container">
                        <div class="row text-center">
                            <!-- Brand Identity -->
                            <div class="col-md-3">
                                <div class="service-card">
                                    <div class="service-icon">
                                        <i class="fa fa-anchor"></i>
                                    </div>
                                    <h3 class="service-title">BRAND IDENTITY</h3>
                                    <p class="service-description">
                                        We’ll help you adapt your brand identity for the digital world -- helping you
                                        improve your logo and the story behind it.
                                    </p>
                                </div>
                            </div>
                            <!-- Web Development -->
                            <div class="col-md-3">
                                <div class="service-card">
                                    <div class="service-icon">
                                        <i class="fa fa-desktop"></i>
                                    </div>
                                    <h3 class="service-title">WEB DEVELOPMENT</h3>
                                    <p class="service-description">
                                        We develop websites from scratch -- tailored to fit our client's needs,
                                        including content and multi-language if needed.
                                    </p>
                                </div>
                            </div>
                            <!-- Mobile Applications -->
                            <div class="col-md-3">
                                <div class="service-card">
                                    <div class="service-icon">
                                        <i class="fa fa-mobile-alt"></i>
                                    </div>
                                    <h3 class="service-title">MOBILE APPLICATIONS</h3>
                                    <p class="service-description">
                                        We enrich your digital experience with powerful iPhone and Android applications
                                        to maximize your sales – putting you just a fingertip away.
                                    </p>
                                </div>
                            </div>
                            <!-- Online Marketing -->
                            <div class="col-md-3">
                                <div class="service-card">
                                    <div class="service-icon">
                                        <i class="fa fa-play"></i>
                                    </div>
                                    <h3 class="service-title">ONLINE MARKETING</h3>
                                    <p class="service-description">
                                        We will make more customers fall for you with our well-planned and strategized
                                        online marketing solutions.
                                    </p>
                                </div>
                            </div>
                        </div>
                    </div>
                </section>

                <section class="stats-section">
                    <div class="stat-card">
                        <div class="icon">
                            <i class="fa-solid fa-folder-open"></i>
                        </div>
                        <div class="stat-number">358</div>
                        <div class="stat-label">projects completed</div>
                    </div>
                    <div class="stat-card">
                        <div class="icon">
                            <i class="fa-solid fa-lightbulb"></i>
                        </div>
                        <div class="stat-number">269</div>
                        <div class="stat-label">concept created</div>
                    </div>
                    <div class="stat-card">
                        <div class="icon">
                            <i class="fa-solid fa-user"></i>
                        </div>
                        <div class="stat-number">157</div>
                        <div class="stat-label">happy clients</div>
                    </div>
                    <div class="stat-card">
                        <div class="icon">
                            <i class="fa-solid fa-trophy"></i>
                        </div>
                        <div class="stat-number">23</div>
                        <div class="stat-label">awards winning</div>
                    </div>
                </section>

            </t>
        </template>
    </data>
</odoo>
